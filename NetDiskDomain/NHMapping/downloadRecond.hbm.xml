<?xml version="1.0"  encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="NetDiskDomain" namespace="NetDiskDomain">
	<class name="NetDiskDomain.DownloadRecond, NetDiskDomain" table="downloadRecond">
		<id name="_id" column="_id" type="int" unsaved-value="0">
      			<generator class="native" />       			
      	</id>      		
		<property name="userId" column="userId" type="int"  />
		<property name="downloadTime" column="downloadTime" type="DateTime"  />
    <!--<property name="nodeId" column="nodeId" type="int"  />-->
    <many-to-one name="Node" class="NetDiskDomain.Node" lazy="false" column="nodeId" unique="true"></many-to-one>
	</class>
</hibernate-mapping>